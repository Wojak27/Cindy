{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Electron: Main (ts-node/esm, Node 22)",
            "type": "pwa-node",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "node",
            "runtimeArgs": [
                "--loader",
                "ts-node/esm"
            ],
            "program": "${workspaceFolder}/node_modules/electron/cli.js",
            "args": [
                "${workspaceFolder}/src/main/main.ts",
                "--remote-debugging-port=9222"
            ],
            "env": {
                "TS_NODE_PROJECT": "${workspaceFolder}/tsconfig.json"
            },
            "smartStep": true,
            "autoAttachChildProcesses": true,
            "console": "integratedTerminal"
        },
        {
            "name": "Electron: Renderer (attach 9222)",
            "type": "pwa-chrome",
            "request": "attach",
            "port": 9222,
            "webRoot": "${workspaceFolder}/src/renderer",
            "timeout": 60000
        },
        {
            "name": "Electron: Main",
            "type": "node",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "/Users/karwo09/.nvm/versions/node/v22.18.0/bin/node",
            "program": "${workspaceFolder}/node_modules/electron/cli.js",
            "args": [
                "${workspaceFolder}/src/main/main.ts",
                "--remote-debugging-port=9222"
            ],
            "console": "integratedTerminal",
            "sourceMaps": true,
            "smartStep": true,
            "resolveSourceMapLocations": [
                "${workspaceFolder}/**",
                "!**/node_modules/**"
            ],
            "outputCapture": "std",
            "env": {
                "NODE_ENV": "development",
                "NODE_OPTIONS": "--max-old-space-size=8192 --require ts-node/register/transpile-only",
                "TS_NODE_COMPILER_OPTIONS": "{\"sourceMap\":true,\"inlineSources\":true}",
                "ELECTRON_ENABLE_LOGGING": "1"
            },
            "autoAttachChildProcesses": true,
            "skipFiles": [
                "<node_internals>/**",
                "node_modules/**"
            ]
        },
        {
            "name": "Electron: Renderer",
            "type": "chrome",
            "request": "attach",
            "port": 9222,
            "webRoot": "${workspaceFolder}",
            "timeout": 60000,
            "sourceMapPathOverrides": {
                "webpack:///./src/*": "${workspaceFolder}/src/*",
                "webpack:///*": "${workspaceFolder}/*"
            }
        },
        {
            "name": "Attach to Renderer (9222)",
